{
  "name": "@dinify/dashboard",
  "version": "0.0.4",
  "private": true,
  "description": "Dinify Dashboard",
  "author": "Lukas Vanek",
  "scripts": {
    "start-dev": "react-scripts start",
    "start": "serve -s build",
    "build": "react-scripts build && cp build/index.html build/200.html",
    "deploy": "react-scripts build && mv ./build/index.html ./build/200.html && surge ./build --domain=https://dashboard.dinify.app",
    "test": "CI=true react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "coverage": "jest --coverage --env=jsdom",
    "lint": "eslint --ext=jsx --ext=js src/",
    "format": "prettier --write '**/*.{js,jsx}'"
  },
  "dependencies": {
    "@dinify/common": "^0.0.4",
    "@material-ui/core": "^3.9.1",
    "@material-ui/icons": "^3.0.2",
    "@material-ui/lab": "^3.0.0-alpha.28",
    "date-fns": "2.0.0-alpha.7",
    "firebase": "^5.8.2",
    "grid-styled": "4.0.0",
    "material-ui-pickers": "1.0.0-rc.17",
    "moment": "2.21.0",
    "normalize-path": "^2.1.1",
    "numeral": "^2.0.6",
    "object-diff": "^0.0.4",
    "polished": "1.9.2",
    "prop-types": "^15.6.2",
    "qrcode.react": "0.7.2",
    "ramda": "0.23.0",
    "raven-for-redux": "^1.3.1",
    "raven-js": "^3.26.4",
    "react": "^16.4.1",
    "react-addons-shallow-compare": "^15.6.2",
    "react-dates": "15.5.3",
    "react-datetime": "^2.14.0",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-dom": "^16.4.1",
    "react-dropzone": "4.2.9",
    "react-enhanced-form": "^1.4.5",
    "react-google-maps": "^9.4.5",
    "react-images-upload": "^1.2.0",
    "react-localization": "0.1.10",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.2.6",
    "react-router": "4.1.1",
    "react-router-dom": "4.1.1",
    "react-select": "^1.2.1",
    "react-sortable-hoc": "^0.6.8",
    "react-switch-button": "2.2.2",
    "react-table": "^6.8.6",
    "react-tap-event-plugin": "2.0.1",
    "recompose": "0.27.0",
    "redux": "^3.7.2",
    "redux-form": "^7.4.2",
    "redux-logger": "3.0.6",
    "redux-observable": "^1.0.0",
    "redux-persist": "^4.10.2",
    "redux-undo": "^0.6.1",
    "reselect": "3.0.1",
    "rxjs": "^6.4.0",
    "serve": "6.5.5",
    "string": "^3.3.3",
    "styled-components": "2.0.1",
    "validator": "8.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "prettier": "^1.5.1",
    "react-scripts": "^2.1.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
