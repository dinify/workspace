<mj-section padding="24px 0px 8px 0px">
  <mj-column>
    <mj-divider/>
  </mj-column>
</mj-section>
<mj-section padding="8px 0px 0px 0px">
  <mj-column>
    <mj-text align="center" mj-class="social">
      <span>
        <a style="padding-right: 4px; text-decoration: none;"
           href="http://www.facebook.com/dinifyapp">
          <img src="https://static.dinify.app/icons/facebook.png"
               width="24px"
               height="24px"
               style="opacity: 0.38"/>
        </a></span>
      <span>
        <a style="padding: 0px 4px; text-decoration: none;"
           href="https://www.instagram.com/dinifyapp">
          <img src="https://static.dinify.app/icons/instagram.png"
               width="24px"
               height="24px"
               style="opacity: 0.38"/>
        </a>
      </span>
      <span>
        <a style="padding: 0px 4px; text-decoration: none;"
           href="https://twitter.com/dinifyapp">
          <img src="https://static.dinify.app/icons/twitter.png"
               width="24px"
               height="24px"
               style="opacity: 0.38"/>
        </a>
      </span>
      <span>
        <a style="padding: 0px 4px; text-decoration: none;"
           href="https://www.dinify.app/">
          <img src="https://static.dinify.app/icons/public.png"
               width="24px"
               height="24px"
               style="opacity: 0.38"/>
        </a>
      </span>
      <span>
        <a style="padding: 0px 4px; text-decoration: none;"
           href="https://github.com/dinify">
