{
  "name": "tabbdashboard",
  "version": "1.0.0",
  "description": "Universal React solution based on React Native and Create React App.",
  "author": "Lukas Vanek",
  "scripts": {
    "start-native-cli": "node node_modules/react-native/local-cli/cli.js start",
    "start-ios": "react-native run-ios --simulator=\"iPhone 7\"",
    "start-android": "react-native run-android",
    "start-web": "react-scripts start",
    "start": "serve -s build",
    "build": "react-scripts build",
    "test": "jest --watch --env=jsdom",
    "coverage": "jest --coverage --env=jsdom",
    "lint": "eslint --ext=jsx --ext=js src/",
    "flow": "flow",
    "precommit": "jest --env=jsdom && flow && lint-staged"
  },
  "dependencies": {
    "grid-styled": "4.0.0",
    "material-ui": "^0.20.0",
    "material-ui-icons": "1.0.0-beta.36",
    "moment": "2.21.0",
    "native-base": "2.3.0",
    "normalize-path": "^2.1.1",
    "numeral": "^2.0.6",
    "polished": "1.9.2",
    "prop-types": "^15.6.1",
    "qrcode.react": "0.7.2",
    "ramda": "0.23.0",
    "react": "16.2.0",
    "react-addons-shallow-compare": "^15.6.2",
    "react-dates": "15.5.3",
    "react-datetime": "^2.14.0",
    "react-dnd": "^2.6.0",
    "react-dnd-html5-backend": "^2.6.0",
    "react-dom": "^16.3.2",
    "react-dropzone": "4.2.9",
    "react-enhanced-form": "^1.4.5",
    "react-form": "^1.3.0",
    "react-google-maps": "^9.4.5",
    "react-images-upload": "^1.1.1",
    "react-localization": "0.1.10",
    "react-native": "0.42.0",
    "react-redux": "^5.0.7",
    "react-router": "4.1.1",
    "react-router-dom": "4.1.1",
    "react-sortable-hoc": "^0.6.8",
    "react-switch-button": "2.2.2",
    "react-table": "^6.8.2",
    "react-tap-event-plugin": "2.0.1",
    "recompose": "0.27.0",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-logger": "3.0.6",
    "redux-observable": "^0.18.0",
    "redux-persist": "^4.10.2",
    "reselect": "3.0.1",
    "rxjs": "^5.5.10",
    "serve": "6.5.5",
    "styled-components": "2.0.1",
    "validator": "8.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react-native": "1.9.2",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "fetch-mock": "5.11.0",
    "flow-bin": "0.46.0",
    "husky": "0.13.3",
    "jest": "20.0.1",
    "jest-fetch-mock": "1.1.1",
    "lint-staged": "3.4.1",
    "prettier-eslint-cli": "4.0.4",
    "react-scripts": "1.0.10",
    "react-test-renderer": "15.4.1",
    "redux-mock-store": "1.2.3"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-eslint --write \"src/*/*.js\"",
      "eslint",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "<rootDir>/flowFixModules.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|@shoutem/theme|@shoutem/animation|@shoutem/ui)"
    ],
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "preset": "react-native"
  }
}
