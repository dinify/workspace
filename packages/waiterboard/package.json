{
  "name": "@dinify/waiterboard",
  "version": "0.1.1",
  "private": true,
  "description": "Dinify Waiterboard",
  "author": "Lukas Vanek",
  "homepage": "https://waiterboard.dinify.app",
  "scripts": {
    "start-dev": "react-scripts start",
    "start": "serve -s build",
    "build": "react-scripts build",
    "deploy": "react-scripts build && firebase deploy --only hosting:dinify-waiterboard",
    "test": "jest --watch --env=jsdom",
    "coverage": "jest --coverage --env=jsdom",
    "lint": "eslint --ext=jsx --ext=js src/",
    "flow": "flow",
    "precommit": "jest --env=jsdom && flow && lint-staged"
  },
  "dependencies": {
    "@dinify/common": "^0.1.1",
    "@material-ui/core": "^3.9.2",
    "@material-ui/icons": "^3.0.2",
    "firebase": "^5.8.2",
    "material-ui-pickers": "1.0.0-rc.17",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "polished": "^1.9.2",
    "ramda": "^0.26.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-form": "^1.3.0",
    "react-redux": "^5.1.1",
    "react-redux-firebase": "^2.2.6",
    "react-router-dom": "^4.3.1",
    "react-swipeable": "^4.2.2",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-observable": "^1.0.0",
    "redux-persist": "^4.10.2",
    "reselect": "^4.0.0",
    "rxjs": "^6.4.0",
    "serve": "9.1.0",
    "socket.io-client": "^2.1.1",
    "string": "^3.3.3",
    "styled-components": "2.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "prettier": "^1.5.1",
    "react-scripts": "^2.1.8"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-eslint --write \"src/*/*.js\"",
      "eslint",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
