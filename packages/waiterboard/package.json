{
  "name": "@dinify/waiterboard",
  "version": "0.1.1",
  "private": true,
  "description": "Dinify Waiterboard",
  "author": "Lukas Vanek",
  "homepage": "https://waiterboard.dinify.app",
  "scripts": {
    "start-dev": "react-scripts start",
    "start": "serve -s build",
    "build": "react-scripts build",
    "deploy": "react-scripts build && firebase deploy --only hosting:dinify-waiterboard",
    "test": "jest --watch --env=jsdom",
    "coverage": "jest --coverage --env=jsdom",
    "lint": "eslint --ext=jsx --ext=js src/",
    "flow": "flow",
    "precommit": "jest --env=jsdom && flow && lint-staged"
  },
  "dependencies": {
    "@dinify/common": "^0.1.1",
    "@material-ui/core": "3.9.1",
    "@material-ui/icons": "^3.0.2",
    "firebase": "^5.8.2",
    "material-ui-pickers": "1.0.0-rc.17",
    "moment": "^2.22.1",
    "numeral": "^2.0.6",
    "polished": "^1.9.2",
    "ramda": "^0.26.0",
    "react": "16.6.0",
    "react-dom": "16.6.0",
    "react-form": "^1.3.0",
    "react-redux": "^5.0.7",
    "react-redux-firebase": "^2.2.6",
    "react-router-dom": "^4.3.1",
    "react-swipeable": "^4.2.2",
    "redux": "3.6.0",
    "redux-logger": "3.0.1",
    "redux-observable": "^1.0.0",
    "redux-persist": "4.8.0",
    "reselect": "^4.0.0",
    "rxjs": "^6.4.0",
    "serve": "^6.5.8",
    "socket.io-client": "^2.1.1",
    "string": "^3.3.3",
    "styled-components": "2.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.4",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "prettier": "^1.5.1",
    "react-scripts": "^2.1.3"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-eslint --write \"src/*/*.js\"",
      "eslint",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
