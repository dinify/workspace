{
  "name": "@dinify/waiterboard",
  "version": "0.0.9",
  "description": "Universal React solution based on React Native and Create React App.",
  "author": "Lukas Vanek",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start-native-cli": "node node_modules/react-native/local-cli/cli.js start",
    "start-ios": "react-native run-ios --simulator=\"iPhone 7\"",
    "start-android": "react-native run-android",
    "start-web": "react-scripts start",
    "start": "serve -s build",
    "build": "react-scripts build && cp build/index.html build/200.html",
    "deploy": "react-scripts build && mv ./build/index.html ./build/200.html && surge ./build --domain=https://waiterboard.gotabb.com",
    "test": "jest --watch --env=jsdom",
    "coverage": "jest --coverage --env=jsdom",
    "lint": "eslint --ext=jsx --ext=js src/",
    "flow": "flow",
    "precommit": "jest --env=jsdom && flow && lint-staged"
  },
  "dependencies": {
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "^1.0.0",
    "expo": "^27.0.2",
    "grid-styled": "^2.0.0-12",
    "material-ui-icons": "^1.0.0-beta.36",
    "moment": "^2.22.1",
    "native-base": "^2.4.5",
    "normalize-path": "^2.1.1",
    "numeral": "^2.0.6",
    "polished": "^1.9.2",
    "ramda": "^0.26.0",
    "rc-slider": "^8.6.1",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-enhanced-form": "^1.4.5",
    "react-fela": "^5.1.1",
    "react-form": "^1.3.0",
    "react-masonry-component": "^6.2.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-27.0.0.tar.gz",
    "react-redux": "5.0.4",
    "react-redux-firebase": "^1.4.6",
    "react-router": "4.1.1",
    "react-router-dom": "4.1.1",
    "react-swipeable": "^4.2.2",
    "react-switch-button": "^2.3.3",
    "react-tap-event-plugin": "2.0.1",
    "redux": "3.6.0",
    "redux-logger": "3.0.1",
    "redux-observable": "0.14.1",
    "redux-persist": "4.8.0",
    "remote-redux-devtools": "0.5.10",
    "reselect": "^4.0.0",
    "rxjs": "5.4.0",
    "serve": "^6.5.8",
    "socket.io-client": "^2.1.1",
    "string": "^3.3.3",
    "styled-components": "2.0.1",
    "styled-props": "^1.1.2",
    "tabb-front": "git+ssh://git@gitlab.com:tabb/frontend-web/common.git#master",
    "validator": "^8.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.4",
    "eslint": "4.18.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "prettier": "^1.5.1",
    "fetch-mock": "5.11.0",
    "flow-bin": "0.46.0",
    "husky": "0.13.3",
    "jest": "20.0.1",
    "jest-fetch-mock": "1.1.1",
    "lint-staged": "3.4.1",
    "react-scripts": "1.0.10",
    "react-test-renderer": "15.4.1",
    "redux-mock-store": "1.2.3"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier-eslint --write \"src/*/*.js\"",
      "eslint",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "<rootDir>/flowFixModules.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|@shoutem/theme|@shoutem/animation|@shoutem/ui)"
    ],
    "automock": false,
    "setupFiles": [
      "./setupJest.js"
    ],
    "preset": "react-native"
  }
}
